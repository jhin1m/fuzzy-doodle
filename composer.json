{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "blade-ui-kit/blade-icons": "^1.5",
    "google/recaptcha": "^1.3",
    "guzzlehttp/guzzle": "^7.2",
    "hammerstone/fast-paginate": "^1.0",
    "intervention/image": "^2.7",
    "iredux/laravel-installer": "dev-master",
    "laravel-lang/common": "^3.1",
    "laravel/fortify": "^1.17",
    "laravel/framework": "^10.10",
    "laravel/sanctum": "^3.2",
    "laravel/socialite": "^5.6",
    "laravel/tinker": "^2.8",
    "opcodesio/log-viewer": "^2.4",
    "owenvoke/blade-fontawesome": "^2.2",
    "php-flasher/flasher-laravel": "^1.13",
    "pion/laravel-chunk-upload": "^1.5",
    "qirolab/laravel-themer": "^2.1",
    "rawilk/laravel-settings": "^2.2",
    "spatie/laravel-backup": "^8.1",
    "spatie/laravel-honeypot": "^4.3",
    "spatie/laravel-permission": "^5.10",
    "spatie/laravel-sitemap": "^6.3",
    "weidner/goutte": "^2.3"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.8",
    "dbrekelmans/bdi": "^1.0",
    "fakerphp/faker": "^1.9.1",
    "laravel/pint": "^1.0",
    "laravel/sail": "^1.18",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^7.0",
    "phpunit/phpunit": "^10.1",
    "spatie/laravel-ignition": "^2.0",
    "symfony/panther": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/",
      "App\\Plugins\\": "plugins"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
